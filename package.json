  "scripts": {
    "heroku-deploy": "npm install --dev && webpack --progress",
    "build": "ts-node --project webpack/tsconfig.webpack.json node_modules/webpack/bin/webpack.js --mode=production --progress --config webpack.config.ts",
    "start": "ts-node --project webpack/tsconfig.webpack.json node_modules/webpack/bin/webpack.js --mode=development --watch --progress --config webpack.config.ts",
    "start-proxy": "ts-node --project api_mock/tsconfig.json api_mock/index.ts",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --quiet --fix && echo 'Lint complete.'",
    "stylelint:fix": "stylelint ./src/**/*.less --syntax less --color --formatter verbose --fix",
    "stylelint:check": "stylelint ./src/**/*.less --syntax less --color --formatter verbose",
    "tsc": "node --max-old-space-size=4096 node_modules/typescript/bin/tsc --noEmit -p tsconfig.json",
    "webpack-bundle-analyzer:prod": "ts-node --project webpack/tsconfig.webpack.json node_modules/webpack/bin/webpack.js --mode=production --progress --config webpack.config.ts --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "webpack-bundle-analyzer:dev": "ts-node --project webpack/tsconfig.webpack.json node_modules/webpack/bin/webpack.js --mode=development --progress --config webpack.config.ts --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "lint:staged": "lint-staged && echo 'Lint staged complete.'",
    "generateIcons": "npx @svgr/cli --typescript --out-dir src/Icons/ReactComponents src/Icons/svgs && git add src/Icons/ReactComponents"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged && npm run tsc"
    }
  },
  "lint-staged": {
    "src/**/*.less": [
      "stylelint *.less --syntax less --color --formatter verbose --fix"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
